{
  "name": "floer-soloer",
  "version": "1.0.0",
  "description": "Template for FloerSoloer, by FloerSoloer",
  "license": "ISC",
  "author": "FloerSoloer",
  "repository": "https://github.com/FloerSoloer/FloerSoloer",
  "private": true,
  "main": "src/index.ts",
  "engines": {
    "node": "^16.9.1"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "prettier --write --loglevel silent . && eslint --fix .",
    "prepare": "husky install",
    "serve": "yarn run build & yarn start",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/index.js",
    "start:dev": "node --loader ts-node/esm src/index.ts",
    "test": "jest",
    "build": "yarn run build:public && yarn run build:src",
    "build:public": "tsc -p public-ts/tsconfig.emit.json",
    "build:src": "tsc -p src/tsconfig.emit.json",
    "commit": "cz",
    "dev": "nodemon"
  },
  "dependencies": {
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.0",
    "node": "^16.11.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "tsc-files": "^1.1.2",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3",
    "yarn": "^1.22.15"
  }
}
