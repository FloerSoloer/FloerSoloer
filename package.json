{
  "name": "expo-template",
  "description": "Expo's React Native template, enhanced",
  "version": "1.0.0",
  "author": "FloerSoloer",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "commit": "cz",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:prettier": "prettier --cache --loglevel silent --ignore-path .gitignore --write \"**/*.{json,md}\"",
    "lint:eslint": "eslint --cache --cache-location node_modules/.cache/eslint/.eslintcache --fix --ignore-path .gitignore \"**/*.{js,ts,tsx}\"",
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:web": "expo start --web"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/stack": "^6.2.3",
    "expo": "~46.0.9",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.1.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "expo-cli": "^6.0.5",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "~4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
